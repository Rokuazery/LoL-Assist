<Window x:Class="LoL_Assist_WAPP.Views.Windows.MainWIndow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LoL_Assist_WAPP.Views.Windows"
        xmlns:viewModels ="clr-namespace:LoL_Assist_WAPP.ViewModels"
        mc:Ignorable="d" 
        Title="LoL Assist" Height="450" Width="800">
    <Window.InputBindings>
        <MouseBinding MouseAction="LeftClick" Command="{Binding MoveWindowCommand}"/>
    </Window.InputBindings>
    <Window.DataContext>
        <viewModels:MainViewModel/>
    </Window.DataContext>
    <WindowChrome.WindowChrome>
        <WindowChrome GlassFrameThickness="0" CornerRadius="0" CaptionHeight="0" UseAeroCaptionButtons="False" ResizeBorderThickness="5"/>
    </WindowChrome.WindowChrome>
    <Grid>
      
    </Grid>
</Window>
